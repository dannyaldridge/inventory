<div class="row">
  <table class="large-3 columns">
    <thead>
      <tr>
        <th>Item Type Name</th>
      </tr>
      <tbody>
        <% @itemtypes.each do |itemtype| %>
          <tr>
            <td class="row large-6 columns">
              <%= link_to itemtype.name, itemtype %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </thead>
  </table>
</div>
<div class="row">
  <%= link_to "Add a new item type", new_itemtype_path%>
</div>  